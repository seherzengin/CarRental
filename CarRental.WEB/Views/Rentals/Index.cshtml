@using CarRental.Caching
@using CarRental.Repository.Models
@{
    ViewData["Title"] = "Index";
}
@model IEnumerable<RentalDto>;
<table class="table table-striped">
        <thead>
            <tr>
            <td>Id</td>
            <td>CustomerId</td>
            <td>CarsId</td>
            <td>RentDate</td>
            <td>RetumDate</td>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
               <td>@item.Id</td>
                <td>@item.CustomerId</td>
                 <td>@item.CarsId</td>
                  <td>@item.RentDate</td>
                   <td>@item.RetumDate</td>
                      <td>
                        <a class="btn btn-warning" asp-controller="Rentals" asp-action="Update" asp-route-id="@item.Id">Update</a>
                          <a class="btn btn-danger" asp-controller="Rentals" asp-action="Remove" asp-route-id="@item.Id">Delete</a>
                        </td>
                 </tr>
            }

        </tbody>



    </table>
