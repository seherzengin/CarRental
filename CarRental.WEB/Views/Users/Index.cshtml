@using CarRental.Caching
@using CarRental.Repository.Models
@{
    ViewData["Title"] = "Index";
}
@model IEnumerable<UserDto>;
<table class="table table-striped">
        <thead>
            <tr>
            <td>Id</td>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Email</td>
            <td>PasswordHash</td>
            <td>PasswordSalt</td>
            <td>Status</td>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
               <td>@item.Id</td>
                <td>@item.FirstName</td>
                 <td>@item.LastName</td>
                  <td>@item.Email</td>
                   <td>@item.PasswordHash</td>
                    <td>@item.PasswordSalt</td>
                     <td>@item.Status</td>
                      <td>
                        <a class="btn btn-warning" asp-controller="Users" asp-action="Update" asp-route-id="@item.Id">Update</a>
                          <a class="btn btn-danger" asp-controller="Users" asp-action="Remove" asp-route-id="@item.Id">Delete</a>
                        </td>
                 </tr>
            }

        </tbody>



    </table>

