@using CarRental.Service
@using CarRental.Repository.Models
@{
    ViewData["Title"] = "Index";
}
@model IEnumerable<CustomerDto>;
<table class="table table-striped">
        <thead>
            <tr>
            <td>Id</td>
            <td>UserId</td>
            <td>CompanyName</td>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
               <td>@item.Id</td>
                <td>@item.UserId</td>
                 <td>@item.CompanyName</td>
                      <td>
                        <a class="btn btn-warning" asp-controller="Customers" asp-action="Update" asp-route-id="@item.Id">Update</a>
                          <a class="btn btn-danger" asp-controller="Customers" asp-action="Remove" asp-route-id="@item.Id">Delete</a>
                        </td>
                 </tr>
            }

        </tbody>



    </table>

